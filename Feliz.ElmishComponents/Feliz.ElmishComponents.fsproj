<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Description>Elmish components as React elements without the boilerplate based on the Feliz API</Description>
        <PackageIconUrl></PackageIconUrl>
        <PackageTags>fsharp;fable;react;html</PackageTags>
        <Authors>Zaid Ajaj</Authors>
        <Version>1.2.0</Version>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageReleaseNotes>Relax FSharp.Core dependency</PackageReleaseNotes>
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="Components.fs" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\Feliz.UseElmish\Feliz.UseElmish.fsproj" />
        <ProjectReference Include="..\Feliz\Feliz.fsproj" />
    </ItemGroup>
    <ItemGroup>
        <Content Include="*.fsproj; *.fs; *.js;" PackagePath="fable\" />
    </ItemGroup>
</Project>